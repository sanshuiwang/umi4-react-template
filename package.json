{
  "name": "umi4-react-template",
  "private": true,
  "author": "sanshuiwang <768188667@qq.com>",
  "scripts": {
    "build": "umi build --clean",
    "dev": "umi dev",
    "dev:no-mock": "MOCK=none umi dev",
    "postinstall": "umi setup",
    "prepare": "husky install",
    "server": "node server --title=UMI4_REACT_TEMPLATE",
    "server:local": "nodemon server --title=UMI4_REACT_TEMPLATE",
    "setup": "umi setup",
    "start": "npm run dev:no-mock",
    "umi:g:dva": "umi g dva",
    "umi:g:mock": "umi g mock"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "umi lint"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server/*",
      "dist/*"
    ]
  },
  "dependencies": {
    "antd": "^4.24.2",
    "axios": "^1.1.3",
    "express": "^4.18.2",
    "safe-reaper": "^2.1.0",
    "umi": "^4.0.30"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.32",
    "@umijs/plugins": "^4.0.30",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "typescript": "^4.1.2"
  }
}
